<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:parentTag="androidx.constraintlayout.widget.ConstraintLayout">

    <ImageView
        android:layout_width="@dimen/fixture_team_logo_size"
        android:layout_height="@dimen/fixture_team_logo_size"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/ic_team_away"
        android:id="@+id/ic_team_home" />


    <ImageView
        android:layout_width="@dimen/fixture_team_logo_size"
        android:layout_height="@dimen/fixture_team_logo_size"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/ic_team_home"
        android:id="@+id/ic_team_away" />

</merge>